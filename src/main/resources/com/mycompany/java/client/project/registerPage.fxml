<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane prefHeight="688.0" prefWidth="974.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.java.client.project.RegisterPageController">
    <children>
        <VBox alignment="TOP_CENTER" maxHeight="420.0" maxWidth="390.0" prefHeight="420.0" prefWidth="390.0" spacing="15.0" style="-fx-border-color: black; -fx-border-width: 1; -fx-background-color: white;">
            <children>
                <Label prefHeight="35.0" prefWidth="100.0" style="-fx-font-size: 24;" text="Register" />

                <VBox alignment="CENTER_LEFT" spacing="5.0">
                    <children>
                        <Label graphicTextGap="8.0" style="-fx-font-size: 12;" text="Username" />
                        <TextField fx:id="usernameField" prefHeight="37.0" prefWidth="358.0" style="-fx-border-color: black; -fx-border-width: 1; -fx-background-color: white;" />
                    </children>
                    <VBox.margin>
                        <Insets left="18.0" right="18.0" />
                    </VBox.margin>
                </VBox>

                <VBox alignment="CENTER_LEFT" spacing="5.0">
                    <children>
                        <Label style="-fx-font-size: 12px;" text="Password" />
                        <PasswordField fx:id="passwordField" prefHeight="36.0" prefWidth="358.0" style="-fx-border-color: black; -fx-border-width: 1; -fx-background-color: white;" />
                    </children>
                    <VBox.margin>
                        <Insets left="18.0" right="18.0" />
                    </VBox.margin>
                </VBox>

                <HBox alignment="CENTER_LEFT" spacing="5.0">
                    <children>
                        <Label prefHeight="18.0" prefWidth="272.0" style="-fx-font-size: 12px;" text="Gender" />
                        <ComboBox fx:id="genderComboBox" prefHeight="36.0" prefWidth="110.0" promptText="Select Gender" style="-fx-border-color: black; -fx-border-width: 1; -fx-background-color: white;">
                            <items>
                                <FXCollections fx:factory="observableArrayList">
                                    <String fx:value="Male" />
                                    <String fx:value="Female" />
                                    <String fx:value="Other" />
                                </FXCollections>
                            </items>
                        </ComboBox>
                    </children>
                    <VBox.margin>
                        <Insets left="18.0" right="18.0" />
                    </VBox.margin>
                </HBox>

                <HBox alignment="CENTER" spacing="10.0">
                    <VBox.margin>
                        <Insets top="5.0" />
                    </VBox.margin>
                </HBox>
                <Button fx:id="registerButton" prefHeight="33.0" prefWidth="94.0" style="-fx-font-size: 12; -fx-border-color: black; -fx-border-width: 1; -fx-background-color: white; -fx-border-radius: 3; -fx-background-radius: 3;" text="Register" />

                <HBox alignment="CENTER" spacing="10.0" />
                <Button fx:id="backToLoginButton" onAction="#navigateToLogin" prefHeight="38.0" prefWidth="150.0" style="-fx-font-size: 12; -fx-border-color: black; -fx-border-width: 1; -fx-background-color: white; -fx-border-radius: 3; -fx-background-radius: 3;" text="Back to Login" />
            </children>

            <padding>
                <Insets bottom="20.0" left="0" right="0" top="20.0" />
            </padding>
        </VBox>
    </children>
</StackPane>